@model imgbruh.Features.Imgs.Create.Command
<section id="uploadForm">
    @using (Html.BeginForm("", "imgs", null, FormMethod.Post, new { @class = "form-horizontal", role = "form", enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()
                
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div id="image-upload"></div>
        @Html.ValidationMessageFor(m => m.Image, "", new { @class = "text-danger" })
    }
</section>
